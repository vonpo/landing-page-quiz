{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "settingsStore": ["src/store/settings"],
      "chatStore": ["src/store/chat"]
    },
    "moduleResolution": "node",
    "outDir": "./public/",
    "sourceMap": true,
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true
  }
}
